Index: target/linux/ramips/base-files/etc/board.d/02_network
===================================================================
--- target/linux/ramips/base-files/etc/board.d/02_network	(revision 45974)
+++ target/linux/ramips/base-files/etc/board.d/02_network	(working copy)
@@ -230,6 +230,7 @@
 		ucidef_add_switch_vlan "switch0" "2" "4 6t"
 		;;
 
+	hiwifi-hc5*61 | hiwifi-hc5661a \
 	y1s)
 		ucidef_set_interfaces_lan_wan "eth0.1" "eth0.2"
 		ucidef_add_switch "switch0" "1" "1"
@@ -324,6 +325,13 @@
 		wan_mac=$(macaddr_add "$lan_mac" 1)
 		;;
 
+	hiwifi-hc5*61 | hiwifi-hc5661a)
+		__fac_mac=`strings /dev/mtd8 | grep 'fac_mac = ..:..:..:..:..:..'`
+		lan_mac=`expr "$__fac_mac" : '.*\(..:..:..:..:..:..\)' | tr '[A-Z]' '[a-z]'`
+		[ -n "$lan_mac" ] || lan_mac=$(cat /sys/class/net/eth0/address)
+		wan_mac=$(macaddr_add "$lan_mac" 1)
+		;;
+
 	ht-tm02)
 		lan_mac=$(cat /sys/class/net/eth0/address)
 		;;
